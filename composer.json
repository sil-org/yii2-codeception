{
  "name": "silinternational/yii2-codeception",
  "description": "Patches to implement nice features of yii2-codeception since it was abandoned",
  "license": "MIT",
  "authors": [
    {
      "name": "Phillip Shipley",
      "email": "phillip_shipley@sil.org"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6.0",
    "yiisoft/yii2": "^2.0.5",
    "codeception/codeception": ">=2.2",
    "fillup/fake-bower-assets": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Sil\\Codeception\\": "src/Codeception/",
      "Sil\\yii\\": "src/yii/"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
